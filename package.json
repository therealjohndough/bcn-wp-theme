{
  "name": "bcn-wp-theme",
  "version": "1.0.0",
  "description": "Buffalo Cannabis Network WordPress Theme with enhanced member features",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint assets/js/ --ext .js",
    "lint:fix": "eslint assets/js/ --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "phpcs": "phpcs --standard=WordPress .",
    "phpcbf": "phpcbf --standard=WordPress .",
    "phpstan": "phpstan analyse",
    "quality": "npm run lint && npm run phpcs",
    "quality:fix": "npm run lint:fix && npm run phpcbf",
    "lighthouse": "lighthouse https://staging6.buffalocannabisnetwork.com --output=json --output-path=.cursor/agents/reports/lighthouse.json",
    "deploy:staging": "./scripts/deploy-local.sh staging",
    "deploy:production": "./scripts/deploy-local.sh production",
    "test:theme": "./scripts/test-theme.sh",
    "agents:run": "./.cursor/agents/scripts/agent-runner.sh",
    "agents:watch": "./.cursor/agents/scripts/agent-runner.sh watch",
    "agents:status": "./.cursor/agents/scripts/agent-runner.sh status"
  },
  "keywords": [
    "wordpress",
    "theme",
    "cannabis",
    "members",
    "events",
    "buffalo"
  ],
  "author": "John Dough D'Angelo",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "lighthouse": "^11.3.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}